<%= render 'nav' %>
<%= form_for @book do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :pages %>
  <%= f.text_field :pages %>
  <%= f.label :published %>
  <%= f.text_field :published %>
  <%= f.label :authors %>
  <%= f.text_field :authors, value: '' %>
  <%= f.label :image_url %>
  <%= f.text_field :image_url %>
  <%= f.submit %>
<% end %>
